<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                             http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">

    <servlet>
        <servlet-name>PrediccionServlet</servlet-name>
        <servlet-class>proyecto.arbol.servlet.PrediccionServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>PrediccionServlet</servlet-name>
        <url-pattern>/PrediccionServlet</url-pattern>
    </servlet-mapping>

    <session-config>
        <session-timeout>30</session-timeout>
    </session-config>

</web-app>
